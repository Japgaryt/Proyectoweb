\n      >\n        <template #evento=\"{item}\">\n          <td>\n            <CButton\n                color=\"warning\"\n                size=\"sm\"\n                class=\"m-2\"\n            >\n              Edit.\n            </CButton>\n\n            <CButton\n                color=\"danger\"\n                size=\"sm\"\n                class=\"m-2\"\n                @click=\"toggleDetails(item)\" >\n              Borrar\n            </CButton>\n          </td>\n        </template>\n      </CDataTable>\n    </CCardBody>\n  </CCard>\n</template>\n\n<script>\nexport default {\n  name: 'Table',\n  props: {\n    items: Array,\n    fields: {\n      type: Array,\n        default () {\n         // return ['username', 'registered', 'role', 'status']\n         return ['id_producto','codigo_producto', 'id_proveedor', 'descripcion', 'cantidad','precio_unitario','total_precio','evento']\n      }\n    },\n    caption: {\n      type: String,\n      default: 'Table'\n    },\n    tableFilter:Boolean,\n    hover: Boolean,\n    striped: Boolean,\n    border: Boolean,\n    small: Boolean,\n    fixed: Boolean,\n    dark: Boolean\n  },\n  methods: {\n    getBadge (status) {\n      return status === 'Active' ? 'success'\n        : status === 'Inactive' ? 'secondary'\n          : status === 'Pending' ? 'warning'\n            : status === 'Banned' ? 'danger' : 'primary'\n    }\n  }\n}\n</script>\n"]}]}