AAA,IAAA,WAAA,IAAA,OAAA,KAAA,gBAAA,IAAA,WAAA,IAAA,KAAA,iBAAA,IAAA,CAAA,IAAA,KAAA,cAAA,IAAA,CAAA,EAAA;AACA,aAAA,WAAA;AACA,OAFA,MAEA;AACA,QAAA,QAAA,CAAA,eAAA,GAAA,KAAA,cAAA;AACA,QAAA,QAAA,CAAA,YAAA,GAAA,CAAA;AACA,QAAA,QAAA,CAAA,WAAA,GAAA,KAAA,gBAAA;AACA,QAAA,QAAA,CAAA,QAAA,GAAA,KAAA,iBAAA;AACA,QAAA,QAAA,CAAA,eAAA,GAAA,KAAA,cAAA;AACA,QAAA,QAAA,CAAA,YAAA,GAAA,KAAA,qBAAA;AACA,QAAA,QAAA,CAAA,MAAA,GAAA,CAAA;AAEA,QAAA,KAAA,CAAA,IAAA,CAAA,sCAAA,EAAA,QAAA,EACA,IADA,CACA,UAAA,IAAA,EAAA;AACA,UAAA,KAAA,CAAA,aAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,SAJA;AAKA;AACA,KA1CA;AA4CA,IAAA,kBA5CA,gCA4CA;AAEA,aAAA,KAAA,gBAAA,GAAA,CAAA,CAAA,CAAA;AACA;AA/CA;AAnDA,CAAA","sourcesContent":["<template>\n  <div>\n    <CRow>\n      <CCol  md=\"12\">\n        <CCard>\n          <CCardHeader>\n            <strong>Agregar Producto</strong> Form\n          </CCardHeader>\n          <CCardBody>\n            <CForm class=\"row g-3\">\n              <div class=\"col-md-6\">\n                <CInput\n                    label=\"Codigo Produto\"\n                    valid-feedback=\"Codigo Válido\"\n                    invalid-feedback=\"Proporcione al menos 3 caracteres.\"\n                    :is-valid=\"validator\"\n                />\n              </div>\n              <div class=\"col-md-6\">\n                <CInput\n                    label=\"Cantidad\"\n                    valid-feedback=\"Cantidad Válida\"\n                    invalid-feedback=\"Proporcione al menos 1 número.\"\n                    :is-valid=\"validCantProducto\"\n                    @change=\"cantidadProducto\"\n                />\n              </div>\n              <div class=\"col-md-6\">\n                <CInput\n                    label=\"Descripción\"\n                    valid-feedback=\"OK!\"\n                    invalid-feedback=\"Proporcione al menos 10 caracteres.\"\n                    :is-valid=\"validDescriptProduct\"\n                />\n              </div>\n              <div class=\"col-md-6\">\n                <CInput\n                    label=\"Precio Unitario\"\n                    valid-feedback=\"OK!\"\n                    invalid-feedback=\"Proporcione al menos 1 número.\"\n                    :is-valid=\"validCantProducto\"\n                    @change=\"valorPrecioUnitario\"\n                />\n              </div>\n              <div class=\"col-md-6\">\n                <CSelect\n                    label=\"Proveedor\"\n                    :options=\"[1,2,3,4,5,6,7,8,9]\"\n                />\n              </div>\n\n              <div class=\"col-md-6\">\n                <CInput\n                    label=\"Total Q.00\"\n                    :value.sync=\"precioTotal\"\n                    disabled\n                />\n              </div>\n              <div class=\"col-md-6\">\n                <CButton\n                    type=\"submit\"\n                    size=\"sm\"\n                    color=\"info\"\n                    @click=\"enviarDataNuevoProducto\"\n                >\n                  <CIcon name=\"cil-plus\"/>\n                  Agregar\n                </CButton>\n\n                <CButton type=\"reset\" size=\"sm\" color=\"danger\"><CIcon name=\"cil-ban\"/> Reset</CButton>\n              </div>\n\n\n              <CToaster :autohide=\"3000\">\n                <template v-for=\"toast in fixedToasts\">\n                  <CToast\n                      :key=\"'toast' + toast\"\n                      :show=\"true\"\n                      color=\"warning\"\n                      header=\"Información\"\n                  >\n                    Debe completar el formulario.\n                  </CToast>\n                </template>\n              </CToaster>\n              <CToaster :autohide=\"2000\">\n                <template v-for=\"toast in successToasts\">\n                  <CToast\n                      :key=\"'toast' + toast\"\n                      :show=\"true\"\n                      color=\"success\"\n                      header=\"Productos\"\n                  >\n                   Producto agregado correctamente!\n                  </CToast>\n                </template>\n              </CToaster>\n\n            </CForm>\n          </CCardBody>\n        </CCard>\n      </CCol>\n    </CRow>\n\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nlet ff = ['3434']\nexport default {\n  name: 'AgregarProductos',\n  data () {\n    return {\n      fixedToasts: 0,\n      successToasts: 0,\n      codigoProducto:0,\n      descriptProducto:0,\n      cantidadProductos: 0,\n      precioUnitario: 0,\n      precioTotal:0,\n      precioTotalSinFormato:0,\n      selected: [], // Must be an array reference!\n      listProveedores: ff,\n      show: true,\n      horizontal: { label:'col-3', input:'col-9' },\n      options: ['Option 1', 'Option 2', 'Option 3'],\n\n      selectOptions: [\n        'Option 1', 'Option 2', 'Option 3',\n        { \n          value: 'some value', \n          label: 'Selected option'\n        }\n      ],\n      selectedOption: 'some value',\n\n      formCollapsed: true,\n      checkboxNames: ['Checkboxes', 'Inline Checkboxes',\n                      'Checkboxes - custom', 'Inline Checkboxes - custom'],\n      radioNames: ['Radios', 'Inline Radios',\n                    'Radios - custom', 'Inline Radios - custom']\n    }\n  },\n  mounted () {\n    let array = [];\n    this.codigo_proveedor = [];\n    axios.get('https://localhost:5001/api/Proveedores')\n        .then(resp => {\n          resp.data.forEach(proveedor =>\n                //this.codigo_proveedor.push(proveedor.codigo_proveedor)\n              //console.log(arr.push(proveedor.codigo_proveedor))\n              array.push(proveedor.codigo_proveedor)\n              //this.codigo_proveedor.push(proveedor.codigo_proveedor)\n          );\n          //this.codigo_proveedor = resp.data[0].codigo_proveedor;\n          //console.log(resp.data[0].codigo_proveedor);\n        });\n\n    console.log(array);\n  },\n  methods: {\n    validator (val) {\n      this.codigoProducto = val;\n      return val ? val.length >= 4 : false\n    },\n    validDescriptProduct (val) {\n      this.descriptProducto = val;\n      return val ? val.length >= 10 : false\n    },\n    validCantProducto(val) {\n      return val ? val.length >= 1 : false\n    },\n    cantidadProducto(val){\n      this.cantidadProductos = val;\n      this.precioTotalSinFormato = this.cantidadProductos * this.precioUnitario;\n      this.precioTotal = new Intl.NumberFormat('en-IN', { maximumSignificantDigits: 3 }).format(this.cantidadProductos * this.precioUnitario)\n    },\n    valorPrecioUnitario(val){\n      this.precioUnitario = val;\n      this.precioTotalSinFormato = this.precioUnitario * this.cantidadProductos;\n      this.precioTotal = new Intl.NumberFormat('en-IN', { maximumSignificantDigits: 3 }).format(this.precioUnitario * this.cantidadProductos)\n    },\n\n    enviarDataNuevoProducto(){\n      let dataSent = new Object();\n      if((typeof this.codigoProducto == 'undefined' ||  typeof this.descriptProducto == 'undefined') || (  this.cantidadProductos == 0 ||   this.precioUnitario == 0)){\n        this.fixedToasts++;\n      }else{\n        dataSent.codigo_producto = this.codigoProducto;\n        dataSent.id_proveedor = 1;\n        dataSent.descripcion = this.descriptProducto;\n        dataSent.cantidad = this.cantidadProductos;\n        dataSent.precio_unitario = this.precioUnitario;\n        dataSent.total_precio = this.precioTotalSinFormato;\n        dataSent.evento = 1;\n\n        axios.post('https://localhost:5001/api/Productos', dataSent)\n            .then(resp => {\n              this.successToasts++;\n              console.log(resp.data);\n            });\n      }\n    },\n\n    listProveeroresFun(){\n\n      return this.codigo_proveedor = [2];\n    }\n\n  }\n}\n\n\n</script>\n"],"sourceRoot":"src/views/base"}]}