header-actions\">\n              <a \n                href=\"https://coreui.io/vue/docs/components/button-components\" \n                class=\"card-header-action\" \n                rel=\"noreferrer noopener\" \n                target=\"_blank\"\n              >\n                <small class=\"text-muted\">docs</small>\n              </a>\n            </div>\n        </CCardHeader>\n        <CCardBody>\n          <small>Usage </small>\n          <code>{{ $options.usage }}</code>\n          <hr/>\n          <h6>\n            Size Small\n            <small>Add <code>size=\"sm\"</code></small>\n          </h6>\n          <p>\n            <template v-for=\"(brandName, key) in $options.brands\">\n              <CButton\n                :name=\"brandName\"\n                size=\"sm\"\n                :key=\"key\"\n                :color=\"brandName\"\n                class=\"btn-brand\"\n              >\n                <CIcon size=\"sm\" :name=\"'cib-' + brandName\"/>\n                <span>{{brandName}}</span>\n              </CButton>\n            </template>\n          </p>\n          <h6>Size Normal</h6>\n          <p>\n            <template v-for=\"(brandName, key) in $options.brands\">\n              <CButton\n                :name=\"brandName\"\n                :key=\"key\"\n                :color=\"brandName\"\n                class=\"btn-brand\"\n              >\n                <CIcon :name=\"'cib-' + brandName\"/>\n                <span>{{brandName}}</span>\n              </CButton>\n            </template>\n          </p>\n          <h6>Size Large <small>Add <code>size=\"lg\"</code></small></h6>\n          <p>\n            <template v-for=\"(brandName, key) in $options.brands\">\n              <CButton\n                :name=\"brandName\"\n                size=\"lg\"\n                :key=\"key\"\n                :color=\"brandName\"\n                class=\"btn-brand\"\n              >\n                <CIcon size=\"lg\" :name=\"'cib-' + brandName\"/>\n                <span>{{brandName}}</span>\n              </CButton>\n            </template>\n          </p>\n        </CCardBody>\n      </CCard>\n    </CCol>\n    <CCol col=\"12\">\n      <CCard>\n        <CCardHeader>\n          <strong>Brand Button </strong> <small>Icons only</small>\n        </CCardHeader>\n        <CCardBody>\n          <small>Usage </small>\n            <code>{{ $options.iconsOnlyUsage }}</code>\n          <hr/>\n          <h6>Size Small <small>Add <code>size=\"sm\"</code></small></h6>\n          <p>\n            <template v-for=\"(brandName, key) in $options.brands\">\n              <CButton\n                :name=\"brandName\"\n                size=\"sm\"\n                :key=\"key\"\n                :color=\"brandName\"\n              >\n                <CIcon size=\"sm\" :name=\"'cib-' + brandName\"/>\n              </CButton>\n            </template>\n          </p>\n          <h6>Size Normal</h6>\n          <p>\n            <template v-for=\"(brandName, key) in $options.brands\">\n              <CButton\n                :name=\"brandName\"\n                :key=\"key\"\n                :color=\"brandName\"\n              >\n                <CIcon :name=\"'cib-' + brandName\"/>\n              </CButton>\n            </template>\n          </p>\n          <h6>Size Large <small>Add <code>size=\"lg\"</code></small></h6>\n          <p>\n            <template v-for=\"(brandName, key) in $options.brands\">\n              <CButton\n                :name=\"brandName\"\n                size=\"lg\"\n                :key=\"key\"\n                :color=\"brandName\"\n              >\n                <CIcon size=\"lg\" :name=\"'cib-' + brandName\"/>\n              </CButton>\n            </template>\n          </p>\n        </CCardBody>\n      </CCard>\n    </CCol>\n\n    <CCol col=\"12\">\n      <CCard>\n        <CCardHeader>\n          <strong>Brand Button </strong> <small>Text only</small>\n        </CCardHeader>\n        <CCardBody>\n          <small>Usage </small> \n          <code>\n            {{ $options.textOnlyUsage }}\n          </code>\n          <hr/>\n          <h6>Size Small <small>Add <code>size=\"sm\"</code></small></h6>\n          <p>\n            <template v-for=\"(brandName, key) in $options.brands\">\n              <CButton\n                size=\"sm\"\n                :key=\"key\"\n                :color=\"brandName\"\n              >\n                <span>{{brandName}}</span>\n              </CButton>\n            </template>\n          </p>\n          <h6>Size Normal</h6>\n          <p>\n            <template v-for=\"(brandName, key) in $options.brands\">\n              <CButton\n                :key=\"key\"\n                :color=\"brandName\"\n              >\n                <span>{{brandName}}</span>\n              </CButton>\n            </template>\n          </p>\n          <h6>Size Large <small>Add <code>size=\"lg\"</code></small></h6>\n          <p>\n            <template v-for=\"(brandName, key) in $options.brands\">\n              <CButton\n                size=\"lg\"\n                :key=\"key\"\n                :color=\"brandName\"\n              >\n                <span>{{brandName}}</span>\n              </CButton>\n            </template>\n          </p>\n        </CCardBody>\n      </CCard>\n    </CCol>\n  </CRow>\n</template>\n\n<script>\nexport default {\n  name: 'BrandButtons',\n  usage: '<CButton color=\"facebook\"><CIcon name=\"cib-facebook\"/><span>Facebook</span></CButton>',\n  iconsOnlyUsage: '<CButton color=\"facebook\"><CIcon name=\"cib-facebook\"/></CButton>',\n  textOnlyUsage: '<CButton color=\"facebook\"><span>Facebook</span></CButton>',\n  brands: [\n    'facebook',\n    'twitter',\n    'linkedin',\n    'flickr',\n    'tumblr',\n    'xing',\n    'github',\n    'stackoverflow',\n    'youtube',\n    'dribbble',\n    'instagram',\n    'pinterest',\n    'vk',\n    'yahoo',\n    'behance',\n    'reddit',\n    'vimeo'\n  ]\n  // labels: {\n  //   facebook: 'Facebook',\n  //   twitter: 'Twitter',\n  //   linkedin: 'LinkedIn',\n  //   flickr: 'Flickr',\n  //   tumblr: 'Tumblr',\n  //   xing: 'Xing',\n  //   github: 'Github',\n  //   stackoverflow: 'StackOverflow',\n  //   youtube: 'YouTube',\n  //   dribbble: 'Dribbble',\n  //   instagram: 'Instagram',\n  //   pinterest: 'Pinterest',\n  //   vk: 'VK',\n  //   yahoo: 'Yahoo',\n  //   behance: 'Behance',\n  //   reddit: 'Reddit',\n  //   vimeo: 'Vimeo'\n  // }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.btn {\n  margin-bottom: 4px;\n  margin-right: 6px;\n}\n// todo\n.btn-brand:not(:only-child) {\n  .c-icon {\n    margin-top: 0 !important;\n    margin-right: 8px;\n  }\n  .c-icon-sm {\n    margin-right: 5px;\n  }\n  .c-icon-lg {\n    margin-right: 12px;\n  }\n}\n</style>\n"]}]}